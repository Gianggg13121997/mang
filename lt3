    <script>
        let number = [4, 5, 23, 55, 11, 66, 47, 1, 3, 32];
        let maxnb = number[0];
        let sum = 0
        let trungbinh = number.length
        for (i = 0; i < number.length; i++) {
            sum = sum + number[i]
        }
        if (number[i] < maxnb) {
            maxnb = number[i]
        }console.log (" trung bình của dãy số là :" +(sum/trungbinh))
        console.log (" số lớn nhất là "+ maxnb)

    </script>

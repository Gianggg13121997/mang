        let number = [ 4,-5,23,-55,11,-66.6,47,1,3,32];
        let dem = 0
        for ( i = 0 ; i < number.length; i++){
            if(number[i]<0){
                dem++
            }
        }console.log("số âm có trong dãy là " +dem)
    </script>

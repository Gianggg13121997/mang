    <script>
        let number = [ 4,5,23,55,11,66,4,1,3,32]
        let dem = 0
        for ( let i = 0 ; i < number.length ; i++){
            if( number[i] >= 10){
              dem++
            }
        } console.log("các số lớn hơn 10 :" + dem + " số")
    </script>
</body>

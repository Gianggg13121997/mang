        let v = +prompt("nhập số v :")
        let number = [4, 5, 23, 55, 11, 66, 47, 1, 3, 32];
        for ( i =0; i< number.length ; i++){
            if(number[i]==v){
                alert(" V is in the array");
                break
            }else{
                alert("V is not in the array");
                break
            }
        }
    </script>
